struct Vec3 {
	X float64
	Y float64
	Z float64
}

struct Move {
	Source		[16]byte
	Target		[16]byte
	Position	Vec3
}

struct Attack {
	Source	[16]byte
	Target	[16]byte
}

struct Heal {
	Source	[16]byte
	Target	[16]byte
}

struct SetEntity {
	Source		[16]byte
	Type		uint8
	Position	Vec3
}

struct SetPC {
	Type		uint8
}

struct Message {
	Token		[16]byte
	ACK			*[16]byte
	TS			int64
	Action union {
		Move
		Attack
		Heal
		SetEntity
		SetPC
	}
}
