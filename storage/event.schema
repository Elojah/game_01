struct DamageReceived {
	Source	[16]byte
	Amount	int64
}

struct DamageDone {
	Target	[16]byte
	Amount	int64
}

struct HealReceived {
	Source	[16]byte
	Amount	int64
}

struct HealDone {
	Target	[16]byte
	Amount	int64
}

struct Event {
	ID	[16]byte
	TS	int64
	Action union {
		DamageReceived
		DamageDone
		HealReceived
		HealDone
	}
}
