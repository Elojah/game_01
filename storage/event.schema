struct Vec3 {
	X		float64
	Y		float64
	Z		float64
}

struct Move {
	Source	[16]byte
	Target	[16]byte
	Position Vec3
}

struct Skill {
	ID		[16]byte
	Source	[16]byte
	Target	[16]byte
	Position Vec3
}

struct SetPC {
	Type uint8
}

struct ConnectPC {
	Target		[16]byte
}

struct Event {
	ID		[16]byte
	Source	[16]byte
	TS		int64
	Action union {
		Move
		Skill
		ConnectPC
		SetPC
	}
}
