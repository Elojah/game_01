struct Vec3 {
	X		float64
	Y		float64
	Z		float64
}

struct Move {
	Source	[16]byte
	Target	[16]byte
	Position Vec3
}

struct Cast {
	AbilityID	[16]byte
	Source		[16]byte
	Targets		[][16]byte
	Position	Vec3
}

struct SetPC {
	Type	[16]byte
}

struct ConnectPC {
	Target	[16]byte
}

struct Event {
	ID		[16]byte
	Source	[16]byte
	TS		int64
	Action union {
		Move
		Cast
		ConnectPC
		SetPC
	}
}
