struct Move {
	X		float64
	Y		float64
	Z		float64
	Target	[16]byte
}

struct AttackReceived {
	Source	[16]byte
	Target	[16]byte
}

struct AttackDone {
	Source	[16]byte
	Target	[16]byte
}

struct HealReceived {
	Source	[16]byte
	Target	[16]byte
}

struct HealDone {
	Source	[16]byte
	Target	[16]byte
}

struct Event {
	ID		[16]byte
	Source	[16]byte
	TS		int64
	Action union {
		Move
		AttackReceived
		AttackDone
		HealReceived
		HealDone
	}
}
