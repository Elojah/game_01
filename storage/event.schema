struct Damage {
	Source	[16]byte
	Amount	int64
}

struct DamageInflict {
	Target	[16]byte
	Amount	int64
}

struct Heal {
	Source	[16]byte
	Amount	int64
}

struct HealInflict {
	Target	[16]byte
	Amount	int64
}

struct Event {
	ID	[16]byte
	TS	int64
	Action union {
		Damage
		DamageInflict
		Heal
		HealInflict
	}
}
